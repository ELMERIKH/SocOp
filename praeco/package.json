  },
  "nyc": {
    "instrument": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "register": "babel-register",
    "include": [
      "apollo-server/**/*.js",
      "src/**/*.{js,vue}"
